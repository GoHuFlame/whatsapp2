{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "source": "/styles.css",
      "destination": "/api/styles.css"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
